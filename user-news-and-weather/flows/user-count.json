{
  "schemaVersion": "2",
  "name": "user-count",
  "start": "model.count",
  "nodes": {
    "model.count": {
      "type": "nodehandler://api-builder-flow-invoke/model/User",
      "method": "count",
      "name": "Count Users",
      "parameters": [
        {
          "name": "limit",
          "value": "$.params.limit"
        },
        {
          "name": "skip",
          "value": "$.params.skip"
        },
        {
          "name": "where",
          "value": "$.params.where"
        },
        {
          "name": "order",
          "value": "$.params.order"
        }
      ],
      "outputs": {
        "next": {
          "context": "$.count",
          "routes": [
            "response.success"
          ]
        }
      }
    },
    "response.success": {
      "type": "nodehandler://axway-flow/http",
      "name": "Set response",
      "method": "setresponse",
      "parameters": [
        {
          "name": "status",
          "value": "200",
          "type": "number"
        },
        {
          "name": "body",
          "value": "$.count"
        }
      ],
      "outputs": {
        "next": {
          "context": "$.response"
        }
      }
    }
  }
}
